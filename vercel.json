{
  "buildCommand": "cd mobile && npm install && npx expo export --platform web",
  "outputDirectory": "mobile/dist",
  "installCommand": "echo 'Skipping root install'",
  "framework": null,
  "functions": {
    "api/ftp-tunnel.js": {
      "maxDuration": 30
    },
    "api/ftp-discover.js": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/ftp-panel",
      "dest": "/api/ftp-panel"
    },
    {
      "src": "/ftp",
      "dest": "/api/ftp-tunnel/"
    },
    {
      "src": "/admin",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}